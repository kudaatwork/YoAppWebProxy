@model YoAppWebProxy.ViewModels.AuditTrailViewModel

<script src="/build/js/custom.min.js"></script>


    <div class="card-box table-responsive">
        <table id="datatable-buttons" class="table table-striped table-bordered " style="width:100%">
            <thead>
                <tr>
                    <th>DateTime Logged</th>
                    <th>Log Type</th>
                    <th>Service Provider</th>
                    <th>Request/Response Object</th>
                    <th>Actions</th>
                </tr>
            </thead>


            <tbody>

                @if (Model.Logs != null)
                {
                    foreach (var item in Model.Logs)
                    {
                        <tr>
                            <td>@item.DateTimeLogged</td>
                            <td>@item.LogType</td>
                            <td>@item.ServiceProvider</td>
                            <td>@item.RequestResponse</td>
                            <td class=" last">
                                <!--<a href="#"><i class="fa fa-eye"></i> View</a>-->
                                <button type="button" class="btn btn-default" data-toggle="modal" data-target=".bs-example-modal-lg"><i class="fa fa-eye"></i> View</button>
                            </td>
                        </tr>
                    }
                }

            </tbody>
        </table>
    </div>


    <!-- Bootstrap -->
    <script src="/vendors/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <!-- FastClick -->
    <script src="/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="/vendors/nprogress/nprogress.js"></script>
    <!-- Chart.js -->
